<h1>Add Your Song!</h1>

<form method= "post" action="/songs">
  <div>
    <label>Song:</label>
    <input type="text" name="title">
  </div>
  <div>
    <label>Singer:</label>
    <input type="text" name="author">
  </div>
  <div>
    <label>url (optional):</label>
    <input type="text" name="url">
  </div>
  <div>
    <input type="submit" value="Submit">
  </div>
</form>
